	file = {Versión enviada:C\:\\Users\\G731\\Zotero\\storage\\CXY22QHP\\Möemke y Svensson - 2011 - Approximating Graphic TSP by Matchings.pdf:application/pdf;Snapshot:C\:\\Users\\G731\\Zotero\\storage\\CCV28L4K\\12OmNqFJhTW.html:text/html},
