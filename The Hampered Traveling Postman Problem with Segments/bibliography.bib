\_eprint: https://doi.org/10.1080/10556788.2011.648932},
	keywords = {90C26, 90C30, 90C57, non-convex and nonlinear optimization, spatial branch-and-bound, travelling salesman problem with neighbourhoods},
	pages = {364--378},
	file = {Snapshot:C\:\\Users\\G731\\Zotero\\storage\\KKGW9PH8\\10556788.2011.html:text/html;Full Text PDF:C\:\\Users\\G731\\Zotero\\storage\\JBQ9BISV\\Gentilini et al. - 2013 - The travelling salesman problem with neighbourhood.pdf:application/pdf},
